@{
    ViewBag.Title = "DC_OA_PurchaseDeposit";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-6 lr-form-item"  data-table="DC_OA_PurchaseDeposit" >
        <div class="lr-form-item-title">采购项目<font face="宋体">*</font></div>
        <input id="F_PurchaseName" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_OA_PurchaseDeposit" >
        <div class="lr-form-item-title">项目编号<font face="宋体">*</font></div>
        <input id="F_PurchaseProjectNo" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_OA_PurchaseDeposit" >
        <div class="lr-form-item-title">采购单位</div>
        <input id="F_CurrentCompanyId" type="text" readonly class="form-control currentInfo lr-currentInfo-company" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_OA_PurchaseDeposit" >
        <div class="lr-form-item-title">当前信息</div>
        <input id="F_CurrentDeptId" type="text" readonly class="form-control currentInfo lr-currentInfo-department" />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_OA_PurchaseDeposit" >
        <div class="lr-form-item-title">保证金额</div>
        <input id="F_DepositMoney" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item lr-form-item-grid" >
        <div id="DC_OA_PurchaseDepositDetail"></div>
    </div>
</div>
@Html.AppendJsFile("/Areas/LR_CodeDemo/Views/DC_OA_PurchaseDeposit/Form.js")

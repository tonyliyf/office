    <script type="text/javascript">
    	$(".yc_an").click(function(){
			var wid=$(".yc_tcc").css("right");
			if(wid=="10px"){
				$(".yc_tcc").animate({
				right: '-175px'
				}, 300);
			} else{
				$(".yc_tcc").animate({
				right: '10px'
				}, 300);
			}
		});
		$(".arr_jt").click(function () {
            var wid = $(".xsp").width();
            if (wid == 16.4) {
                $(".xsp").animate({
                    width: '260px'
                }, 100);
                $(this).removeClass("ind_la").addClass("ind_shang");
            } else {
                $(".xsp").animate({
                    width: '38px'
                }, 100);
                $(this).removeClass("ind_shang").addClass("ind_la");
            }
        });
    </script>

@{
    ViewBag.Title = "DC_ASSETS_EquipmentPartsInfo";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_EquipmentPartsInfo" >
        <div class="lr-form-item-title">编 码</div>
        <input id="F_PartsCode" type="text" readonly class="form-control" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_EquipmentPartsInfo" >
        <div class="lr-form-item-title">配件名称</div>
        <input id="F_PartsName" type="text" class="form-control"  />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_EquipmentPartsInfo" >
        <div class="lr-form-item-title">配件类型<font face="宋体">*</font></div>
        <div id="F_PartsType" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_EquipmentPartsInfo" >
        <div class="lr-form-item-title">规格型号</div>
        <input id="F_SpecificationType" type="text" class="form-control"  />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_EquipmentPartsInfo" >
        <div class="lr-form-item-title">计量单位</div>
        <input id="F_MeasurementUnit" type="text" class="form-control"  />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_EquipmentPartsInfo" >
        <div class="lr-form-item-title">单价</div>
        <input id="F_UnitPrice" type="text" class="form-control" isvalid="yes" checkexpession="NumOrNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_EquipmentPartsInfo" >
        <div class="lr-form-item-title">生产厂商</div>
        <div id="F_Manufacturer"  ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_EquipmentPartsInfo" >
        <div class="lr-form-item-title">经销商</div>
        <div id="F_Distributor"  ></div>
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_ASSETS_EquipmentPartsInfo" >
        <div class="lr-form-item-title">存放位置</div>
        <input id="F_StorageLocation" type="text" class="form-control"  />
    </div>
    <div class="col-xs-4 lr-form-item"  data-table="DC_ASSETS_EquipmentPartsInfo" >
        <div class="lr-form-item-title">初始库存<font face="宋体">*</font></div>
        <input id="F_InitialInventory" type="text" class="form-control" isvalid="yes" checkexpession="Num" />
    </div>
    <div class="col-xs-4 lr-form-item"  data-table="DC_ASSETS_EquipmentPartsInfo" >
        <div class="lr-form-item-title">最大库存</div>
        <input id="F_MaximumInventory" type="text" class="form-control" isvalid="yes" checkexpession="NumOrNull" />
    </div>
    <div class="col-xs-4 lr-form-item"  data-table="DC_ASSETS_EquipmentPartsInfo" >
        <div class="lr-form-item-title">最小库存</div>
        <input id="F_MinimumInventory" type="text" class="form-control" isvalid="yes" checkexpession="NumOrNull" />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_ASSETS_EquipmentPartsInfo" >
        <div class="lr-form-item-title">图片附件</div>
        <div id="F_PictureAccessories"  ></div>
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_ASSETS_EquipmentPartsInfo" >
        <div class="lr-form-item-title">关联设备</div>
        <div id="RelativeDevice"  ></div>
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_ASSETS_EquipmentPartsInfo" >
        <div class="lr-form-item-title">配件状态<font face="宋体">*</font></div>
        <div id="F_PartsState" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_ASSETS_EquipmentPartsInfo" >
        <div class="lr-form-item-title">备注</div>
        <textarea id="F_Remarks" class="form-control" style="height:100px;"  ></textarea>
    </div>
</div>
@Html.AppendJsFile("/Areas/AssetManager/Views/DC_ASSETS_EquipmentPartsInfo/Form.js")

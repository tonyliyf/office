@{
    ViewBag.Title = "DC_ASSETS_LandHandUpInfo";
    Layout = "~/Views/Shared/_Form.cshtml";
}

<div class="lr-form-tabs" id="lr_form_tabs">
    <ul class="nav nav-tabs">
        <li><a data-value="tab1">基本信息</a></li>
        <li><a data-value="tab2">合同信息</a></li>
        <li><a data-value="tab3">票据信息</a></li>
    </ul>
</div>
<div class="tab-content lr-tab-content" id="lr_tab_content">
    <div class="lr-form-wrap tab-pane" id="tab1">
        <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_LandHandUpInfo">
            <div class="lr-form-item-title">土地证号<font face="宋体">*</font></div>
            <input id="F_LandNo" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_LandHandUpInfo">
            <div class="lr-form-item-title">合同编号<font face="宋体">*</font></div>
            <input id="F_ContractNo" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_LandHandUpInfo">
            <div class="lr-form-item-title">宗地坐落<font face="宋体">*</font></div>
            <input id="F_Address" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_LandHandUpInfo">
            <div class="lr-form-item-title">地类</div>
            <div id="F_LandUseRight"></div>
        </div>
        <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_LandHandUpInfo">
            <div class="lr-form-item-title">土地招拍挂状态</div>
            <div id="F_State"></div>
        </div>
        <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_LandHandUpInfo">
            <div class="lr-form-item-title">面积(m2)<font face="宋体">*</font></div>
            <input id="F_Area" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_LandHandUpInfo">
            <div class="lr-form-item-title">出让金（万元）<font face="宋体">*</font></div>
            <input id="F_TotalMoney" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
        </div>
        <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_LandHandUpInfo">
            <span><i class="fa fa-map-marker"></i></span>
            <input id="btn_area" type="button" class="btn btn-primary lrUploader-input-btn" value="选择区域" />
        </div>
        <div class="col-xs-12 lr-form-item" data-table="DC_ASSETS_LandHandUpInfo">
            <div class="lr-form-item-title">摘牌单位<font face="宋体">*</font></div>
            <div id="F_ManagerDept" isvalid="yes" checkexpession="NotNull"></div>
        </div>
        <div class="col-xs-12 lr-form-item" data-table="DC_ASSETS_LandHandUpInfo">
            <div class="lr-form-item-title">约定开工时间</div>
            <input id="F_StartDate" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_StartDate').trigger('change'); } })" />
        </div>
        <div class="col-xs-12 lr-form-item" data-table="DC_ASSETS_LandHandUpInfo">
            <div class="lr-form-item-title">开工到期时间</div>
            <input id="F_StartEndDate" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_StartEndDate').trigger('change'); } })" />
        </div>
        <div class="col-xs-12 lr-form-item" data-table="DC_ASSETS_LandHandUpInfo">
            <div class="lr-form-item-title">竣工时间</div>
            <input id="F_EndDate" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_EndDate').trigger('change'); } })" />
        </div>
        <div class="col-xs-12 lr-form-item" data-table="DC_ASSETS_LandHandUpInfo">
            <div class="lr-form-item-title">备注</div>
            <textarea id="F_Remarks" class="form-control" style="height:80px;"></textarea>
        </div>
    </div>
    <div class="lr-form-wrap tab-pane" id="tab2">
       
        <div class="col-xs-12 lr-form-item" data-table="DC_ASSETS_LandHandUpInfo">
            <div class="lr-form-item-title">合同名称</div>
            <input id="F_ContractName" type="text" class="form-control" />
        </div>
        <div class="col-xs-12 lr-form-item" data-table="DC_ASSETS_LandHandUpInfo">
            <div class="lr-form-item-title">合同附件</div>
            <div id="F_ContractAccessories"></div>
        </div>
        <div class="col-xs-12 lr-form-item" data-table="DC_ASSETS_LandHandUpInfo">
            <div class="lr-form-item-title">成交确认书附件</div>
            <div id="F_SalesConfirmation"></div>
        </div>
    </div>
    <div class="lr-form-wrap tab-pane" id="tab3">
        <div class="col-xs-12 lr-form-item" data-table="DC_ASSETS_LandHandUpInfo">
            <div class="lr-form-item-title">票据说明</div>
            <input id="F_NoteDescription" type="text" class="form-control" />
        </div>
        <div class="col-xs-12 lr-form-item" data-table="DC_ASSETS_LandHandUpInfo">
            <div class="lr-form-item-title">票据附件</div>
            <div id="F_NoteAccessories"></div>
        </div>
        <div class="col-xs-12 lr-form-item" data-table="DC_ASSETS_LandHandUpInfo">
            <div class="lr-form-item-title">其他资料附件</div>
            <div id="F_OtherAccessories"></div>
        </div>
       
    </div>
 </div>
    @Html.AppendJsFile("/Areas/AssetManager/Views/DC_ASSETS_LandHandUpInfo/Form.js")

@{
    ViewBag.Title = "DC_EngineProject_MeetingRecord";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap">
    <div class="col-xs-12 lr-form-item" data-table="DC_EngineProject_MeetingRecord">
        <div class="lr-form-item-title">会议主题<font face="宋体">*</font></div>
        <input id="F_MeetingTheme" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 lr-form-item" data-table="DC_EngineProject_MeetingRecord">
        <div class="lr-form-item-title">项目名称</div>
        <div id="F_PIId"></div>
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_EngineProject_MeetingRecord">
        <div class="lr-form-item-title">会议编号</div>
        <input id="F_MRNum" type="text" readonly class="form-control" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_EngineProject_MeetingRecord">
        <div class="lr-form-item-title">会议地址</div>
        <input id="F_MeetingAddress" type="text" class="form-control" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_EngineProject_MeetingRecord">
        <div class="lr-form-item-title">召开部门</div>
        <div id="F_ConveningDepartment"></div>
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_EngineProject_MeetingRecord">
        <div class="lr-form-item-title">召集人</div>
        <div id="F_Convenor"></div>
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_EngineProject_MeetingRecord">
        <div class="lr-form-item-title">会议类型</div>
        <div id="F_MRType"></div>
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_EngineProject_MeetingRecord">
        <div class="lr-form-item-title">主持人</div>
        <input id="F_MeetingHost" type="text" class="form-control" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_EngineProject_MeetingRecord">
        <div class="lr-form-item-title">参会单位</div>
        <input id="F_MeetingUnits" type="text" class="form-control" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_EngineProject_MeetingRecord">
        <div class="lr-form-item-title">参会人</div>
        <input id="F_MeetingAttendee" type="text" class="form-control" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_EngineProject_MeetingRecord">
        <div class="lr-form-item-title">开始时间</div>
        <input id="F_StartTime" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd HH:mm',onpicked: function () { $('#F_StartTime').trigger('change'); } })" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_EngineProject_MeetingRecord">
        <div class="lr-form-item-title">结束时间</div>
        <input id="F_EndTime" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd HH:mm',onpicked: function () { $('#F_EndTime').trigger('change'); } })" />
    </div>
    <div class="col-xs-12 lr-form-item" data-table="DC_EngineProject_MeetingRecord">
        <div class="lr-form-item-title">时长</div>
        <input id="F_Duration" type="text" class="form-control" />
    </div>
    <div class="col-xs-12 lr-form-item" data-table="DC_EngineProject_MeetingRecord">
        <div class="lr-form-item-title">会议议题</div>
        <input id="F_MeetingtOpics" type="text" class="form-control" />
    </div>
    <div class="col-xs-12 lr-form-item" data-table="DC_EngineProject_MeetingRecord">
        <div class="lr-form-item-title">会议内容</div>
        <input id="F_MeetingContent" type="text" class="form-control" />
    </div>
    <div class="col-xs-12 lr-form-item" data-table="DC_EngineProject_MeetingRecord">
        <div class="lr-form-item-title">图片</div>
        <div id="F_ScenePictures"></div>
    </div>
    <div class="col-xs-12 lr-form-item" data-table="DC_EngineProject_MeetingRecord">
        <div class="lr-form-item-title">附件</div>
        <div id="F_Attachments"></div>
    </div>
    <div class="col-xs-12 lr-form-item" data-table="DC_EngineProject_MeetingRecord" id="num1">
        <div class="lr-form-item-title">记录部门</div>
        <input id="F_CreateDepartmentId" type="text" readonly class="form-control currentInfo lr-currentInfo-department" />
    </div>
    <div class="col-xs-12 lr-form-item" data-table="DC_EngineProject_MeetingRecord" id="num2">
        <div class="lr-form-item-title">记录人</div>
        <input id="F_CreateUserid" type="text" readonly class="form-control currentInfo lr-currentInfo-user" />
    </div>
    <div class="col-xs-12 lr-form-item" data-table="DC_EngineProject_MeetingRecord" id="num3">
        <div class="lr-form-item-title">记录时间</div>
        <input id="F_CreateDatetime" type="text" readonly class="form-control currentInfo lr-currentInfo-time" />
    </div>
</div>
@Html.AppendJsFile("/Areas/ProjectManager/Views/DC_EngineProject_MeetingRecord/Form.js")

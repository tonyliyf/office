@{
    ViewBag.Title = "DC_ASSETS_HouseRentIncome";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_HouseRentIncome">
        <div class="lr-form-item-title">年度<font face="宋体">*</font></div>
        <input id="F_YearNumber" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_HouseRentIncome">
        <div class="lr-form-item-title">批次<font face="宋体">*</font></div>
        <input id="F_Year" type="text" class="form-control" isvalid="yes" checkexpession="Num" />
    </div>
    <div class="col-xs-12 lr-form-item" data-table="DC_ASSETS_HouseRentIncome">
        <div class="lr-form-item-title">合同编号</div>
        <input id="F_ContractNumber" type="text" class="form-control" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_HouseRentIncome">
        <div class="lr-form-item-title">签约时间</div>
        <input id="F_ContractSignDate" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_ContractSignDate').trigger('change'); } })" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_HouseRentIncome">
        <div class="lr-form-item-title">实际价格<font face="宋体">*</font></div>
        <input id="F_ActualPrice" type="text" class="form-control" isvalid="yes" checkexpession="Num" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_HouseRentIncome">
        <div class="lr-form-item-title">CPI</div>
        <input id="F_CPI" type="text" class="form-control" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_HouseRentIncome">
        <div class="lr-form-item-title">计算公式</div>
        <div id="F_CalculationFormula"></div>
    </div>
    <div class="col-xs-12 lr-form-item" data-table="DC_ASSETS_HouseRentIncome">
        <div class="lr-form-item-title">缴纳银行</div>
        <input id="F_PayingBank" type="text" class="form-control" />
    </div>
    <div class="col-xs-12 lr-form-item" data-table="DC_ASSETS_HouseRentIncome">
        <div class="lr-form-item-title">缴款账号</div>
        <input id="F_PaymentAccount" type="text" class="form-control" />
    </div>
    <div class="col-xs-12 lr-form-item" data-table="DC_ASSETS_HouseRentIncome">
        <div class="lr-form-item-title">缴款时间<font face="宋体">*</font></div>
        <input id="F_PaymentDate" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_PaymentDate').trigger('change'); } })" isvalid="yes" checkexpession="NotNull" />
    </div>
</div>
@Html.AppendJsFile("/Areas/AssetManager/Views/DC_ASSETS_HouseRentIncome/Form.js")

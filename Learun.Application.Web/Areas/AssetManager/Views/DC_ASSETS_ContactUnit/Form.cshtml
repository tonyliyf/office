@{
    ViewBag.Title = "DC_ASSETS_ContactUnit";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_ContactUnit" >
        <div class="lr-form-item-title">单位名称<font face="宋体">*</font></div>
        <input id="F_UnitName" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_ContactUnit" >
        <div class="lr-form-item-title">单位编号</div>
        <input id="F_UnitCode" type="text" class="form-control"  />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_ContactUnit" >
        <div class="lr-form-item-title">单位类型<font face="宋体">*</font></div>
        <div id="F_UnitType" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_ContactUnit" >
        <div class="lr-form-item-title">联系人</div>
        <input id="F_Contacts" type="text" class="form-control"  />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_ContactUnit" >
        <div class="lr-form-item-title">联系电话</div>
        <input id="F_ContactPhone" type="text" class="form-control" isvalid="yes" checkexpession="MobileOrPhoneOrNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_ContactUnit" >
        <div class="lr-form-item-title">传真</div>
        <input id="F_ContactFax" type="text" class="form-control"  />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_ContactUnit" >
        <div class="lr-form-item-title">邮编</div>
        <input id="F_PostCode" type="text" class="form-control"  />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_ContactUnit" >
        <div class="lr-form-item-title">电子邮箱</div>
        <input id="F_ContactEmail" type="text" class="form-control" isvalid="yes" checkexpession="EmailOrNull" />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_ASSETS_ContactUnit" >
        <div class="lr-form-item-title">联系地址</div>
        <input id="F_ContactAddress" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_ASSETS_ContactUnit" >
        <div class="lr-form-item-title">税号</div>
        <input id="F_DutyNumber" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_ASSETS_ContactUnit" >
        <div class="lr-form-item-title">开户行</div>
        <input id="F_DepositBank" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_ASSETS_ContactUnit" >
        <div class="lr-form-item-title">开户行账户</div>
        <input id="F_ContactAccount" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_ASSETS_ContactUnit" >
        <div class="lr-form-item-title">备注</div>
        <textarea id="F_Remarks" class="form-control" style="height:100px;"  ></textarea>
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_ASSETS_ContactUnit" >
        <div class="lr-form-item-title">使用状态<font face="宋体">*</font></div>
        <div id="F_UseState" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
</div>
@Html.AppendJsFile("/Areas/AssetManager/Views/DC_ASSETS_ContactUnit/Form.js")

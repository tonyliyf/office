@{
    ViewBag.Title = "DC_OA__PartyBranch";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-6 lr-form-item"  data-table="DC_OA_PartyBranch" >
        <div class="lr-form-item-title">党组织编号<font face="宋体">*</font></div>
        <input id="F_PartyBranchCode" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_OA_PartyBranch" >
        <div class="lr-form-item-title">党组织名称<font face="宋体">*</font></div>
        <input id="F_PartyBranchName" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_OA_PartyBranch" >
        <div class="lr-form-item-title">上级党组织</div>
        <div id="F_PPartyBranchCode"  ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_OA_PartyBranch" >
        <div class="lr-form-item-title">批准日期</div>
        <input id="F_ApprovalDate" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_ApprovalDate').trigger('change'); } })"   />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_OA_PartyBranch" >
        <div class="lr-form-item-title">届满日期</div>
        <input id="F_ExpiryTime" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_ExpiryTime').trigger('change'); } })"   />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_OA_PartyBranch" >
        <div class="lr-form-item-title">所属企业</div>
        <div id="F_Compnay"  ></div>
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_OA_PartyBranch" >
        <div class="lr-form-item-title">备注</div>
        <textarea id="F_Remark" class="form-control" style="height:100px;"  ></textarea>
    </div>
</div>
@Html.AppendJsFile("/Areas/LR_CodeDemo/Views/DC_OA__PartyBranch/Form.js")

@{
    ViewBag.Title = "DC_ASSETS_HouseInfo";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_HouseInfo">
        <div class="lr-form-item-title">资产编号</div>
        <input id="F_AssetsNumber" type="text" readonly class="form-control" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_HouseInfo">
        <div class="lr-form-item-title">所属建筑<font face="宋体">*</font></div>
        <div id="F_BBIId" isvalid="yes" checkexpession="NotNull"></div>
    </div>

    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_HouseInfo">
        <div class="lr-form-item-title">房屋名称<font face="宋体">*</font></div>
        <input id="F_HouseName" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>


    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_HouseInfo">
        <div class="lr-form-item-title">房屋用途<font face="宋体">*</font></div>
        <div id="F_UseCategories" isvalid="yes" checkexpession="NotNull"></div>
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_HouseInfo">
        <div class="lr-form-item-title">使用性质<font face="宋体">*</font></div>
        <div id="F_RoomUsage" isvalid="yes" checkexpession="NotNull"></div>
    </div>


    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_HouseInfo">
        <div class="lr-form-item-title">房产证号</div>
        <input id="F_CertificateNo" type="text" class="form-control" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_HouseInfo">
        <div class="lr-form-item-title">房屋面积</div>
        <input id="F_HouseArea" type="text" class="form-control" isvalid="yes" checkexpession="NumOrNull" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_HouseInfo">
        <div class="lr-form-item-title">土地面积</div>
        <input id="F_LandArea" type="text" class="form-control" />
    </div>
    <div class="col-xs-12 lr-form-item" data-table="DC_ASSETS_HouseInfo">
        <div class="lr-form-item-title">房产地址</div>
        <input id="F_BuildingAddress" type="text" class="form-control" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_HouseInfo">
        <div class="lr-form-item-title">土地证号</div>
        <input id="F_LandCertificateNo" type="text" class="form-control" />
    </div>

    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_HouseInfo">
        <div class="lr-form-item-title">账面价值(万元)</div>
        <input id="F_BuildingValue" type="text" class="form-control" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_HouseInfo">
        <div class="lr-form-item-title">使用状态</div>
        <div id="F_UseStatus"></div>
    </div>

    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_HouseInfo">
        <div class="lr-form-item-title">所有权人<font face="宋体">*</font></div>
        <div id="F_PropertyOwner" isvalid="yes" checkexpession="NotNull"></div>

    </div>

    @*<div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_HouseInfo">
            <div class="lr-form-item-title">产权人证件类型</div>
            <div id="F_PropertyOwnerCertificateType"></div>
        </div>
        <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_HouseInfo">
            <div class="lr-form-item-title">产权人证件号</div>
            <input id="F_PropertyOwnerCerfificateNo" type="text" class="form-control" />
        </div>*@
    @*<div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_HouseInfo">
            <div class="lr-form-item-title">购置日期</div>
            <input id="F_PurchaseDate" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_PurchaseDate').trigger('change'); } })" />
        </div>*@


    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_HouseInfo">
        <div class="lr-form-item-title">出租用途<font face="宋体">*</font></div>
        <div id="F_RentPurpose" isvalid="yes" checkexpession="NotNull"></div>
    </div>
    @*<div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_HouseInfo">
            <div class="lr-form-item-title">招租年限</div>
            <input id="F_RentAge" type="text" class="form-control" isvalid="yes" checkexpession="NumOrNull" />
        </div>*@
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_HouseInfo">
        <div class="lr-form-item-title">有效状态</div>
        <div id="F_IfUse"></div>
    </div>
    <div class="col-xs-12 lr-form-item" data-table="DC_ASSETS_HouseInfo">
        <div class="lr-form-item-title">相关附件</div>
        <div id="F_PictureAccessories"></div>
    </div>

   

    <div class="col-xs-12 lr-form-item" data-table="DC_ASSETS_HouseInfo">
        <div class="lr-form-item-title">备注</div>
        <textarea id="F_Remarks" class="form-control" style="height:100px;"></textarea>
    </div>
</div>
@Html.AppendJsFile("/Areas/AssetManager/Views/DC_ASSETS_HouseInfo/Form.js")

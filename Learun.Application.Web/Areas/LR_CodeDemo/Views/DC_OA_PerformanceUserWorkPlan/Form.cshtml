@{
    ViewBag.Title = "DC_OA_PerformanceUserWorkPlan";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-12 lr-form-item" data-table="DC_OA_PerformanceUserWorkPlan">
        <div class="lr-form-item-title">指标名称</div>
        <input id="F_TargetName" readonly type="text" class="form-control" />
    </div>
    <div class="col-xs-12 lr-form-item" data-table="DC_OA_PerformanceUserWorkPlan">
        <div class="lr-form-item-title">指标内容</div>
        <input id="F_TargetContent" type="text" class="form-control" />
    </div>
    <div class="col-xs-12 lr-form-item" data-table="DC_OA_PerformanceUserWorkPlan">
        <div class="lr-form-item-title">指标目标</div>
        <input id="F_Target" type="text" class="form-control" />
    </div>
    <div class="col-xs-12 lr-form-item" data-table="DC_OA_PerformanceUserWorkPlan">
        <div class="lr-form-item-title">评分说明</div>
        <input id="F_TargetExplain" type="text" class="form-control" />
    </div>
</div>

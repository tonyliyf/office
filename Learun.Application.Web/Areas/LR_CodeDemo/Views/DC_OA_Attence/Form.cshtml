@{
    ViewBag.Title = "DC_OA_Attence";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-12 lr-form-item"  data-table="DC_OA_Attence" >
        <div class="lr-form-item-title">考勤月份</div>
        <input id="DC_OA_AttenceMonth" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_OA_Attence" >
        <div class="lr-form-item-title">部门</div>
        <div id="F_OA_AttenceDeptId"  ></div>
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_OA_Attence" >
        <div class="lr-form-item-title">姓名</div>
        <input id="F_OA_AttenceUserName" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_OA_Attence" >
        <div class="lr-form-item-title">本月上班天数</div>
        <input id="F_SumDays" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_OA_Attence" >
        <div class="lr-form-item-title">打卡天数</div>
        <input id="F_ComonDays" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_OA_Attence" >
        <div class="lr-form-item-title">请假天数</div>
        <input id="F_LeaveDays" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_OA_Attence" >
        <div class="lr-form-item-title">忘记打卡</div>
        <input id="F_ForgetNums" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_OA_Attence" >
        <div class="lr-form-item-title">补卡次数</div>
        <input id="F_RepairNums" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_OA_Attence" >
        <div class="lr-form-item-title">出差天数</div>
        <input id="F_OutDays" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_OA_Attence" >
        <div class="lr-form-item-title">迟到早退次数</div>
        <input id="F_laterNums" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_OA_Attence" >
        <div class="lr-form-item-title">说明</div>
        <input id="F_Description" type="text" class="form-control"  />
    </div>
</div>
@Html.AppendJsFile("/Areas/LR_CodeDemo/Views/DC_OA_Attence/Form.js")

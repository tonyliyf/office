@{
    ViewBag.Title = "formtable_main_140";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-6 lr-form-item"  data-table="formtable_main_140" >
        <div class="lr-form-item-title">招租单位</div>
        <input id="zzdw" type="text" class="form-control"  />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="formtable_main_140" >
        <div class="lr-form-item-title">经办人</div>

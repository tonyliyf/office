@{
    ViewBag.Title = "DC_ASSETS_EquipmentPartsInto";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_EquipmentPartsInto" >
        <div class="lr-form-item-title">入库单号</div>
        <input id="F_IntoNumber" type="text" readonly class="form-control" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_EquipmentPartsInto" >
        <div class="lr-form-item-title">入库时间<font face="宋体">*</font></div>
        <input id="F_IntoDatetime" type="text" value="@System.DateTime.Now.ToString("yyyy-MM-dd")" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_IntoDatetime').trigger('change'); } })"  isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_EquipmentPartsInto" >
        <div class="lr-form-item-title">入库类型</div>
        <div id="F_IntoType"  ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_EquipmentPartsInto" >
        <div class="lr-form-item-title">使用部门</div>
        <div id="F_UseDepartmentId"  ></div>
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_ASSETS_EquipmentPartsInto" >
        <div class="lr-form-item-title">备注</div>
        <textarea id="F_Remarks" class="form-control" style="height:100px;"  ></textarea>
    </div>
    <div class="col-xs-12 lr-form-item lr-form-item-grid" >
        <div id="DC_ASSETS_EquipmentPartsIntoDetail"></div>
    </div>
</div>
@Html.AppendJsFile("/Areas/AssetManager/Views/DC_ASSETS_EquipmentPartsInto/Form.js")

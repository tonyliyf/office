@{
    ViewBag.Title = "DC_EngineProject_ProjectExaminationSupervise";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-6 lr-form-item"  data-table="DC_EngineProject_ProjectExaminationSupervise" >
        <div class="lr-form-item-title">工程项目</div>
        <div id="F_PIId"  ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_EngineProject_ProjectExaminationSupervise" >
        <div class="lr-form-item-title">检查督办类型</div>
        <div id="F_InspectionSupervisionType"  ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_EngineProject_ProjectExaminationSupervise" >
        <div class="lr-form-item-title">编 码</div>
        <input id="F_PESCode" type="text" readonly class="form-control" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_EngineProject_ProjectExaminationSupervise" >
        <div class="lr-form-item-title">检查时间<font face="宋体">*</font></div>
        <input id="F_EaminationDate" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_EaminationDate').trigger('change'); } })"  isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_EngineProject_ProjectExaminationSupervise" >
        <div class="lr-form-item-title">检查部门<font face="宋体">*</font></div>
        <div id="F_ExaminationDepartment" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_EngineProject_ProjectExaminationSupervise" >
        <div class="lr-form-item-title">检查人员</div>
        <div id="F_ExaminationUser"  ></div>
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_EngineProject_ProjectExaminationSupervise" >
        <div class="lr-form-item-title">检查部位<font face="宋体">*</font></div>
        <input id="F_ExaminationPosition" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_EngineProject_ProjectExaminationSupervise" >
        <div class="lr-form-item-title">检查结果<font face="宋体">*</font></div>
        <input id="F_ExaminationResult" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_EngineProject_ProjectExaminationSupervise" >
        <div class="lr-form-item-title">现场图片</div>
        <div id="F_ScenePictures"  ></div>
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_EngineProject_ProjectExaminationSupervise" >
        <div class="lr-form-item-title">附件资料</div>
        <div id="F_Attachment"  ></div>
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_EngineProject_ProjectExaminationSupervise" >
        <div class="lr-form-item-title">督办意见</div>
        <input id="F_SupervisionOpinion" type="text" class="form-control"  />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_EngineProject_ProjectExaminationSupervise" >
        <div class="lr-form-item-title">是否整改<font face="宋体">*</font></div>
        <div id="F_IfCorrective" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_EngineProject_ProjectExaminationSupervise" >
        <div class="lr-form-item-title">整改时间</div>
        <input id="F_DesignateDate" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_DesignateDate').trigger('change'); } })"   />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_EngineProject_ProjectExaminationSupervise" >
        <div class="lr-form-item-title">指派单位</div>
        <input id="F_DesignateUnit" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_EngineProject_ProjectExaminationSupervise" >
        <div class="lr-form-item-title">指派人员</div>
        <input id="F_DesignateUser" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_EngineProject_ProjectExaminationSupervise" >
        <div class="lr-form-item-title">整改结果反馈</div>
        <input id="F_ResultFeedback" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_EngineProject_ProjectExaminationSupervise" >
        <div class="lr-form-item-title">检查督办状态</div>
        <div id="F_SupervisionStatus"  ></div>
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_EngineProject_ProjectExaminationSupervise" >
        <div class="lr-form-item-title">备注</div>
        <textarea id="F_Remarks" class="form-control" style="height:100px;"  ></textarea>
    </div>
</div>
@Html.AppendJsFile("/Areas/ProjectManager/Views/DC_EngineProject_ProjectExaminationSupervise/Form.js")

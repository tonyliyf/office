@{
    ViewBag.Title = "DC_ASSETS_LandBaseMortgage";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_LandBaseMortgage" >
        <div class="lr-form-item-title">所属土地<font face="宋体">*</font></div>
        <div id="F_LBIId" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_LandBaseMortgage" >
        <div class="lr-form-item-title">处理人<font face="宋体">*</font></div>
        <div id="F_HandleUserId" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_ASSETS_LandBaseMortgage" >
        <div class="lr-form-item-title">抵押单位</div>
        <input id="F_MortgageUnit" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_ASSETS_LandBaseMortgage" >
        <div class="lr-form-item-title">抵押银行<font face="宋体">*</font></div>
        <input id="F_MortgageBank" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_LandBaseMortgage" >
        <div class="lr-form-item-title">抵押起始日期<font face="宋体">*</font></div>
        <input id="F_StartDate" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_StartDate').trigger('change'); } })"  isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_LandBaseMortgage" >
        <div class="lr-form-item-title">抵押结束日期<font face="宋体">*</font></div>
        <input id="F_EndDate" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_EndDate').trigger('change'); } })"  isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_ASSETS_LandBaseMortgage" >
        <div class="lr-form-item-title">说明</div>
        <textarea id="F_HandleDescribe" class="form-control" style="height:100px;"  ></textarea>
    </div>
</div>
@Html.AppendJsFile("/Areas/AssetManager/Views/DC_ASSETS_LandBaseMortgage/Form.js")

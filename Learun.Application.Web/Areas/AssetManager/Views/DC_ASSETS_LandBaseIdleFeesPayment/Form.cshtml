@{
    ViewBag.Title = "DC_ASSETS_LandBaseIdleFeesPayment";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_LandBaseIdleFeesPayment" >
        <div class="lr-form-item-title">所属土地<font face="宋体">*</font></div>
        <div id="F_LBIId" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_LandBaseIdleFeesPayment" >
        <div class="lr-form-item-title">缴费时间<font face="宋体">*</font></div>
        <input id="F_PayDate" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_PayDate').trigger('change'); } })"  isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_ASSETS_LandBaseIdleFeesPayment" >
        <div class="lr-form-item-title">缴费单位</div>
        <input id="F_PayUnit" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_ASSETS_LandBaseIdleFeesPayment" >
        <div class="lr-form-item-title">缴纳银行</div>
        <input id="F_PayBank" type="text" class="form-control"  />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_ASSETS_LandBaseIdleFeesPayment" >
        <div class="lr-form-item-title">缴纳账户</div>
        <input id="F_PayAccount" type="text" class="form-control"  />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_LandBaseIdleFeesPayment" >
        <div class="lr-form-item-title">缴纳金额<font face="宋体">*</font></div>
        <input id="F_PayMoney" type="text" class="form-control" isvalid="yes" checkexpession="Num" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_LandBaseIdleFeesPayment" >
        <div class="lr-form-item-title">处理人</div>
        <div id="F_HandleUserId"  ></div>
    </div>
</div>
@Html.AppendJsFile("/Areas/AssetManager/Views/DC_ASSETS_LandBaseIdleFeesPayment/Form.js")

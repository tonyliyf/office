@{
    ViewBag.Title = "DC_ASSETS_EquipmentMaintenanceRecordsProcess";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_EquipmentMaintenanceRecordsProcess" >
        <div class="lr-form-item-title">维修级别<font face="宋体">*</font></div>
        <div id="F_MaintenanceLevel" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_EquipmentMaintenanceRecordsProcess" >
        <div class="lr-form-item-title">故障类别<font face="宋体">*</font></div>
        <div id="F_FaultClassification" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_EquipmentMaintenanceRecordsProcess" >
        <div class="lr-form-item-title">维修部门<font face="宋体">*</font></div>
        <div id="F_MaintenanceDepartmentId" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_EquipmentMaintenanceRecordsProcess" >
        <div class="lr-form-item-title">负责人<font face="宋体">*</font></div>
        <div id="F_MaintenanceUserId" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_EquipmentMaintenanceRecordsProcess" >
        <div class="lr-form-item-title">维修费用</div>
        <input id="F_MaintenanceCosts" type="text" class="form-control" isvalid="yes" checkexpession="NumOrNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_EquipmentMaintenanceRecordsProcess" >
        <div class="lr-form-item-title">维修日期<font face="宋体">*</font></div>
        <input id="F_MaintenanceDate" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_MaintenanceDate').trigger('change'); } })"  isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_ASSETS_EquipmentMaintenanceRecordsProcess" >
        <div class="lr-form-item-title">故障分析</div>
        <textarea id="F_FaultAnalysis" class="form-control" style="height:100px;"  ></textarea>
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_ASSETS_EquipmentMaintenanceRecordsProcess" >
        <div class="lr-form-item-title">维修说明</div>
        <textarea id="F_MaintenanceInstructions" class="form-control" style="height:100px;"  ></textarea>
    </div>
    <div class="col-xs-12 lr-form-item lr-form-item-grid" >
        <div id="DC_ASSETS_EquipmentMaintenancePartsUse"></div>
    </div>
</div>
@Html.AppendJsFile("/Areas/AssetManager/Views/DC_ASSETS_EquipmentMaintenanceRecordsProcess/Form.js")

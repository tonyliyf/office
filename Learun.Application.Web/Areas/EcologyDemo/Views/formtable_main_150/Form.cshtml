@{
    ViewBag.Title = "formtable_main_150";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-12 lr-form-item"  data-table="formtable_main_150" >
        <div class="lr-form-item-title">项目名称</div>
        <input id="xmmc" type="text" class="form-control"  />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="formtable_main_150" >
        <div class="lr-form-item-title">部门</div>
        <input id="bm" type="text" class="form-control"  />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="formtable_main_150" >
        <div class="lr-form-item-title">月度</div>
        <input id="yd" type="text" class="form-control"  />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="formtable_main_150" >
        <div class="lr-form-item-title">进度（%）</div>
        <input id="jd" type="text" class="form-control"  />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="formtable_main_150" >
        <div class="lr-form-item-title">填写时间</div>
        <input id="txsj" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#txsj').trigger('change'); } })"   />
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="formtable_main_150" >
        <div class="lr-form-item-title">汇报内容</div>
        <textarea id="hbnr" class="form-control" style="height:100px;"  ></textarea>
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="formtable_main_150" >
        <div class="lr-form-item-title">下月计划</div>
        <textarea id="xyjh" class="form-control" style="height:100px;"  ></textarea>
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="formtable_main_150" >
        <div class="lr-form-item-title">备注</div>
        <textarea id="bz" class="form-control" style="height:100px;"  ></textarea>
    </div>
</div>
@Html.AppendJsFile("/Areas/EcologyDemo/Views/formtable_main_150/Form.js")

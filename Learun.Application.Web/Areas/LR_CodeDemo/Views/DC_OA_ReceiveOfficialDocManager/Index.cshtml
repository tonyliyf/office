@{
    ViewBag.Title = "收文管理";
    Layout = "~/Views/Shared/_Index.cshtml";
}
<div class="lr-layout ">
    <div class="lr-layout-center">
        <div class="lr-layout-wrap lr-layout-wrap-notitle ">
            <div class="lr-layout-tool">
                <div class="lr-layout-tool-left">
                    <div class="lr-layout-tool-item">
                        <div id="multiple_condition_query">
                            <div class="lr-query-formcontent">
                                <div class="col-xs-12 lr-form-item">
                                    <div class="lr-form-item-title">文件字号</div>
                                    <input id="F_FileCode" type="text" class="form-control" />
                                </div>
                                <div class="col-xs-12 lr-form-item">
                                    <div class="lr-form-item-title">密级</div>
                                    <div id="F_DenseGrade"></div>
                                </div>
                                <div class="col-xs-12 lr-form-item">
                                    <div class="lr-form-item-title">收文号</div>
                                    <input id="F_ReceiveCode" type="text" class="form-control" />
                                </div>
                                <div class="col-xs-12 lr-form-item">
                                    <div class="lr-form-item-title">来文部室</div>
                                    <div id="F_SenderDepartment"></div>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="lr-layout-tool-right">
                    <div class=" btn-group btn-group-sm">
                        <a id="lr_refresh" class="btn btn-default"><i class="fa fa-refresh"></i></a>
                    </div>
                    <div class=" btn-group btn-group-sm" learun-authorize="yes">
                        <a id="lr_complete" class="btn btn-default"><i class="fa fa-check"></i>&nbsp;归档</a>
                        <a id="lr_info" class="btn btn-default"><i class="fa fa-file"></i>&nbsp;查看文件</a>
                        <a id="lr_print" class="btn btn-default"><i class="fa fa-print"></i>&nbsp;打印处理单</a>
                    </div>
                </div>
            </div>
            <div class="lr-layout-body" id="gridtable"></div>
        </div>
    </div>
</div>
@Html.AppendJsFile("/Areas/LR_CodeDemo/Views/DC_OA_DispatchOfficialDocManager/DocLayer.js", "/Areas/LR_CodeDemo/Views/DC_OA_ReceiveOfficialDocManager/Index.js")

@{
    ViewBag.Title = "DC_ASSETS_HouseRentMain";
    Layout = "~/Views/Shared/_Index.cshtml";
}
<div class="lr-layout lr-layout-left-center" id="lr_layout">
    <div class="lr-layout-left">
        <div class="lr-layout-wrap">
            <div class="lr-layout-title lrlg ">招租租赁计划列表</div>
            <div id="dataTree" class="lr-layout-body"></div>
        </div>
    </div>
    <div class="lr-layout-center">
        <div class="lr-layout-wrap  ">
            <div class="lr-layout-title">
                <span id="titleinfo" class="lrlg">房屋租赁信息</span>
            </div>
            <div class="lr-layout-tool">
                <div class="lr-layout-tool-center">
                    <div class="lr-layout-tool-item">
                        <div id="multiple_condition_query">
                            <div class="lr-query-formcontent">

                                <div class="col-xs-6 lr-form-item">
                                    <div class="lr-form-item-title">所有权人</div>
                                    <div id="F_Assignee"></div>
                                </div>
                                <div class="col-xs-6 lr-form-item">
                                    <div class="lr-form-item-title">原单位</div>
                                    <input id="F_Transferor" type="text" class="form-control" />
                                </div>

                                <div class="col-xs-12 lr-form-item">
                                    <div class="lr-form-item-title">建筑名称</div>
                                    <input id="F_ConstructionName" type="text" class="form-control" />
                                </div>
                                <div class="col-xs-12 lr-form-item">
                                    <div class="lr-form-item-title">房屋名称</div>
                                    <input id="F_HouseName" type="text" class="form-control" />
                                </div>
                                <div class="col-xs-12 lr-form-item">
                                    <div class="lr-form-item-title">坐落地址</div>
                                    <input id="F_Address" type="text" class="form-control" />
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
                <div class="lr-layout-tool-right">
                    <div class=" btn-group btn-group-sm">
                        <a id="lr_refresh" class="btn btn-default"><i class="fa fa-refresh"></i></a>
                    </div>
                    <div class=" btn-group btn-group-sm">
                        <a id="lr_Addcategory" class="btn btn-default"><i class="fa fa-tags"></i>&nbsp;<span class="lrlg">新增招租计划</span></a>
                        <a id="lr_Editcategory" class="btn btn-default"><i class="fa fa-tags"></i>&nbsp;<span class="lrlg">编辑招租计划</span></a>
                        <a id="lr_deletecategory" class="btn btn-default"><i class="fa fa-trash-o"></i>&nbsp;<span class="lrlg">删除招租计划</span></a>
                        <a id="lr_add" class="btn btn-default"><i class="fa fa-eye"></i>&nbsp;新增房屋招租</a>
                        <a id="lr_edit" class="btn btn-default"><i class="fa fa-pencil-square-o"></i>&nbsp;编辑房屋招租</a>
                        <a id="lr_delete" class="btn btn-default"><i class="fa fa-trash-o"></i>&nbsp;<span class="lrlg">删除房屋招租</span></a>
                        <a id="lr_export" class="btn btn-default"><i class="fa fa-eye"></i>&nbsp;导出</a>
                        <a id="lr_file" class="btn btn-default"><i class="fa fa-eye"></i>&nbsp;查看附件</a>
                        @*<a id="lr_importplan" class="btn btn-default"><i class="fa fa-trash-o"></i>&nbsp;导入招租批次</a>*@
                        <a id="lr_import" class="btn btn-default"><i class="fa fa-trash-o"></i>&nbsp;导入租赁信息</a>
                        <a id="lr_importImage" class="btn btn-default"><i class="fa fa-trash-o"></i>&nbsp;导入租赁附件</a>
                    </div>
                </div>
            </div>
            <div class="lr-layout-body" id="gridtable"></div>
        </div>
    </div>
</div>
@Html.AppendJsFile("/Areas/AssetManager/Views/DC_ASSETS_HouseRentMain/IndexFox.js")

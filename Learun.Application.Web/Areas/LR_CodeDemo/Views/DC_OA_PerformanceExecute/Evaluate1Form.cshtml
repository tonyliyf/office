@{
    ViewBag.Title = "DC_OA_PerformanceUserWorkPlan";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-12 lr-form-item" data-table="DC_OA_PerformanceUserWorkPlan">
        <div class="lr-form-item-title">指标名称</div>
        <input id="F_TargetName" readonly type="text" class="form-control" />
    </div>
    <div class="col-xs-12 lr-form-item" data-table="DC_OA_PerformanceUserWorkPlan">
        <div class="lr-form-item-title">指标内容</div>
        <input id="F_TargetContent" readonly type="text" class="form-control" />
    </div>
    <div class="col-xs-12 lr-form-item" data-table="DC_OA_PerformanceUserWorkPlan">
        <div class="lr-form-item-title">指标目标</div>
        <input id="F_Target" readonly type="text" class="form-control" />
    </div>
    <div class="col-xs-12 lr-form-item" data-table="DC_OA_PerformanceUserWorkPlan">
        <div class="lr-form-item-title">评分说明</div>
        <textarea id="F_TargetExplain" readonly class="form-control" style="height:100px;"></textarea>
    </div>
    <div class="col-xs-12 lr-form-item" data-table="DC_OA_PerformanceUserWorkPlan">
        <div class="lr-form-item-title">自评分数</div>
        <input id="F_SelfScore" type="text" class="form-control" isvalid="yes" checkexpession="Num" />
    </div>
    <div class="col-xs-12 lr-form-item" data-table="DC_OA_PerformanceUserWorkPlan">
        <div class="lr-form-item-title">完成情况</div>
        <textarea id="F_PerformanceInfo" class="form-control" style="height:100px;"></textarea>
    </div>
</div>
@Html.AppendJsFile("/Areas/LR_CodeDemo/Views/DC_OA_PerformanceExecute/Evaluate1Form.js")

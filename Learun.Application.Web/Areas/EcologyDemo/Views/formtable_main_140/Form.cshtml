        <div class="lr-form-item-title">招租日期</div>
        <input id="zzrq" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#zzrq').trigger('change'); } })"   />

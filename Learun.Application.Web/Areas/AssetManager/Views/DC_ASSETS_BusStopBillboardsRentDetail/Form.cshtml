@{
    ViewBag.Title = "DC_ASSETS_BusStopBillboardsRentDetail";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_BusStopBillboardsRentDetail">
        <div class="lr-form-item-title">广告牌名称</div>
        <input id="F_BSBId" type="text" class="form-control" readonly />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_BusStopBillboardsRentDetail">
        <div class="lr-form-item-title">评估价</div>
        <input id="F_ValuationPrice" type="text" class="form-control" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_BusStopBillboardsRentDetail">
        <div class="lr-form-item-title">招租底价<font face="宋体">*</font></div>
        <input id="F_RentReservePrice" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_BusStopBillboardsRentDetail">
        <div class="lr-form-item-title">竟租保证金<font face="宋体">*</font></div>
        <input id="F_RentDeposit" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_BusStopBillboardsRentDetail">
        <div class="lr-form-item-title">出租年限<font face="宋体">*</font></div>
        <input id="F_RentAge" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_BusStopBillboardsRentDetail">
        <div class="lr-form-item-title">实际价格<font face="宋体">*</font></div>
        <input id="F_ActualPrice" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_BusStopBillboardsRentDetail">
        <div class="lr-form-item-title">出租面积</div>
        <input id="F_RentArea" type="text" class="form-control" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_BusStopBillboardsRentDetail">
        <div class="lr-form-item-title">租赁状态</div>
        <div id="F_LeaseState"></div>
    </div>
    @*<div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_BusStopBillboardsRentDetail">
            <div class="lr-form-item-title">招标类型</div>
            <div id="F_TenderType"></div>
        </div>*@
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_BusStopBillboardsRentDetail">
        <div class="lr-form-item-title">租赁起始日</div>
        <input id="F_RentStartTime" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_RentStartTime').trigger('change'); } })" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_BusStopBillboardsRentDetail">
        <div class="lr-form-item-title">租赁结束日</div>
        <input id="F_RentEndTime" readonly type="text" class="form-control lr-input-wdatepicker" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_BusStopBillboardsRentDetail">
        <div class="lr-form-item-title">租金提醒</div>
        <input id="F_RentReminder" type="text" class="form-control" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_BusStopBillboardsRentDetail">
        <div class="lr-form-item-title">租金提醒日期</div>
        <input id="F_RentReminderDate" readonly type="text" class="form-control lr-input-wdatepicker" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_BusStopBillboardsRentDetail">
        <div class="lr-form-item-title">租金到期</div>
        <input id="F_ExpireReminder" type="text" class="form-control" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_BusStopBillboardsRentDetail">
        <div class="lr-form-item-title">租金到期日期</div>
        <input id="F_ExpireReminderDate" readonly type="text" class="form-control lr-input-wdatepicker" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_BusStopBillboardsRentDetail">
        <div class="lr-form-item-title">租赁人姓名<font face="宋体">*</font></div>
        <input id="F_Renter" type="text" class="form-control" isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_BusStopBillboardsRentDetail">
        <div class="lr-form-item-title">租赁人单位名称<font face="宋体">*</font></div>
        <input id="F_RenterCompany" type="text" class="form-control"  isvalid="yes" checkexpession="NotNull"/>
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_BusStopBillboardsRentDetail">
        <div class="lr-form-item-title">租赁人联系电话<font face="宋体">*</font></div>
        <input id="F_RenterPhone" type="text" class="form-control"  isvalid="yes" checkexpession="NotNull"/>
    </div>
    <div class="col-xs-6 lr-form-item" data-table="DC_ASSETS_BusStopBillboardsRentDetail">
        <div class="lr-form-item-title">租赁人身份证号<font face="宋体">*</font></div>
        <input id="F_RenterIDNo" type="text" class="form-control"  isvalid="yes" checkexpession="NotNull"/>
    </div>
    <div class="col-xs-12 lr-form-item" data-table="DC_ASSETS_BusStopBillboardsRentDetail">
        <div class="lr-form-item-title">备注</div>
        <textarea id="F_Remarks" class="form-control" style="height:100px;"></textarea>
    </div>
</div>
@Html.AppendJsFile("/Areas/AssetManager/Views/DC_ASSETS_BusStopBillboardsRentDetail/Form.js")

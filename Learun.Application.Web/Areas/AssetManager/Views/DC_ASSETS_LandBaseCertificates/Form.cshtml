@{
    ViewBag.Title = "DC_ASSETS_LandBaseCertificates";
    Layout = "~/Views/Shared/_Form.cshtml";
}
<div class="lr-form-wrap" id="form">
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_LandBaseCertificates" >
        <div class="lr-form-item-title">所属土地<font face="宋体">*</font></div>
        <div id="F_LBIId" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_LandBaseCertificates" >
        <div class="lr-form-item-title">土地证办理状态<font face="宋体">*</font></div>
        <div id="F_CertificatesState" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_LandBaseCertificates" >
        <div class="lr-form-item-title">办理日期<font face="宋体">*</font></div>
        <input id="F_HandleDate" type="text" class="form-control lr-input-wdatepicker" onfocus="WdatePicker({ dateFmt:'yyyy-MM-dd',onpicked: function () { $('#F_HandleDate').trigger('change'); } })"  isvalid="yes" checkexpession="NotNull" />
    </div>
    <div class="col-xs-6 lr-form-item"  data-table="DC_ASSETS_LandBaseCertificates" >
        <div class="lr-form-item-title">办理人<font face="宋体">*</font></div>
        <div id="F_HandleUserId" isvalid="yes" checkexpession="NotNull" ></div>
    </div>
    <div class="col-xs-12 lr-form-item"  data-table="DC_ASSETS_LandBaseCertificates" >
        <div class="lr-form-item-title">说明</div>
        <textarea id="F_HandleDescribe" class="form-control" style="height:100px;"  ></textarea>
    </div>
</div>
@Html.AppendJsFile("/Areas/AssetManager/Views/DC_ASSETS_LandBaseCertificates/Form.js")
